{
  "name": "supascan",
  "version": "0.0.8",
  "description": "Security analysis tool for Supabase databases",
  "license": "MIT",
  "author": "Abhishek Govindarasu",
  "main": "dist/supascan.js",
  "type": "module",
  "bin": {
    "supascan": "dist/supascan.js"
  },
  "files": [
    "dist/supascan.js"
  ],
  "keywords": [
    "supabase",
    "security",
    "analysis",
    "cli",
    "database",
    "rls",
    "postgres",
    "security-audit",
    "penetration-testing",
    "vulnerability-scanner"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhishekg999/supascan.git"
  },
  "homepage": "https://github.com/abhishekg999/supascan#readme",
  "bugs": {
    "url": "https://github.com/abhishekg999/supascan/issues"
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "start": "bun run index.ts",
    "format": "prettier --write .",
    "build": "bun run build:binary && bun run build:bundle",
    "build:binary": "bun run lint && bun build --compile --minify --outfile dist/supascan ./index.ts",
    "build:bundle": "bun run lint && bun build --target node --outfile dist/supascan.js ./index.ts",
    "test": "bun test",
    "prepublishOnly": "bun run build:bundle"
  },
  "devDependencies": {
    "@kitajs/ts-html-plugin": "^4.1.3",
    "@types/bun": "latest",
    "knip": "^5.64.2",
    "prettier": "^3.4.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@supabase/supabase-js": "^2.74.0",
    "commander": "^14.0.1",
    "consola": "^3.4.2",
    "picocolors": "^1.1.1"
  }
}
