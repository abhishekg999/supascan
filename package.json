{
  "name": "supascan",
  "version": "0.1.2",
  "description": "Automated security scanner for Supabase databases - detect exposed data, analyze RLS policies, and test RPC functions",
  "license": "MIT",
  "author": "Abhishek Govindarasu",
  "main": "dist/supascan.js",
  "type": "module",
  "bin": {
    "supascan": "dist/supascan.js"
  },
  "files": [
    "dist/supascan.js"
  ],
  "keywords": [
    "supabase",
    "security",
    "analysis",
    "cli",
    "database",
    "rls",
    "postgres",
    "security-audit",
    "penetration-testing",
    "vulnerability-scanner"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/abhishekg999/supascan.git"
  },
  "homepage": "https://github.com/abhishekg999/supascan#readme",
  "bugs": {
    "url": "https://github.com/abhishekg999/supascan/issues"
  },
  "scripts": {
    "lint": "tsc --noEmit",
    "start": "bun run src/cli/index.ts",
    "format": "prettier --write .",
    "build": "bun run build:binary && bun run build:bundle",
    "build:binary": "bun run lint && bun build --compile --minify --outfile dist/supascan ./src/cli/index.ts",
    "build:bundle": "bun run lint && bun build --minify --target node --outfile dist/supascan.js ./src/cli/index.ts",
    "test": "bun test",
    "prepublishOnly": "bun run build:bundle"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "knip": "^5.64.3",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^14.0.0",
    "@supabase/supabase-js": "^2.75.0",
    "commander": "^14.0.1",
    "picocolors": "^1.1.1",
    "react": "^19",
    "react-dom": "^19"
  }
}
